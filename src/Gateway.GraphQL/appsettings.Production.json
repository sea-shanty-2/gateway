{
  "ForwardedHeaders": {
    "AllowedHosts": [
      "*"
    ],
    "ForwardedHeaders": "XForwardedHost,XForwardedProto"
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 10240,
      "MaxRequestHeaderCount": 20
    }
  },
  "GraphQL": {
    "ComplexityConfiguration": {
      "MaxDepth": 15,
      "MaxComplexity": 1000
    },
    "EnableMetrics": true
  },
  "CacheProfiles": {
    "StaticFiles": {
      "Duration": 31536000,
      "Location": "Any"
    }
  }
}